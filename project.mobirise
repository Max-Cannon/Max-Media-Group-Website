          "title": "How We Design A&nbsp;Highly Effective<br>E-Commerce Website",
          "media": {
            "image": {
              "src": "@PROJECT_PATH@/assets/images/max-media-group-e-commerce-web-design-laptop-1357x1357.png",
              "class": "card-img-top",
              "active": true,
              "link": "max-media-group-ecommerce-website-design-development.html#contacts3-53",
              "alt": "Max Media Group - E-Commerce Web Design Websites Laptop",
              "title": "Max Media Group - E-Commerce Web Design Website Image",
              "linkInNewWindow": false
