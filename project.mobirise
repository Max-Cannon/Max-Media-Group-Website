          "title": "<b>Our Two-Part 'Happiness Guarantee</b>'",
          "meta": "By posted July 20, 2016",
          "_customHTML": "<section class=\"mbr-section content3 mbr-section__container\" style=\"background-color: rgb(255, 255, 255); padding-top: 20px; padding-bottom: 0px;\">\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n                <h3 class=\"mbr-section-title display-4 mbr-editable-content\" code-editable-id=\"474\"><strong>Our Two-Part 'Happiness Guarantee</strong>'</h3>\n                \n            </div>\n        </div>\n    </div>\n</section>",
          "_PHPplaceholders": [],
          "_JSplaceholders": [],
          "_customCSS": ".mbr-section-title, .mbr-section-subtitle {\n\ttext-align: center;\n}\n.display-4 {\n\tcolor: #003464;\n\tfont-size: 36px;\n}\n",
